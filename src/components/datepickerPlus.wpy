    props = {
      // 控制是否显示日期选择组件的flag
      showDatePickerPlus: {
        type: Boolean, // 类型（必填），目前接受的类型包括：String, Number, Boolean, Object, Array, null（表示任意类型）
        default: false, // 属性初始值（可选），如果未指定则会根据类型选择一个
        // 属性被改变时执行的函数（可选），也可以写成在methods段中定义的方法名字符串, 如：'_propertyChange'
      },
      // initDate: {
      //   type: String,
      //   default: formatTime(new Date())
      // }
    };
    // 私有数据，可用于模版渲染
    data = {
      datePickerAttr: {
        showYear: 0,
        dateType: 1,
      },
      initDate: `${new Date().getFullYear()}-${new Date().getMonth() + 1}-${new Date().getDate()}`,
      nlDateMutiIndex: [90, 0, 0],
      nlDateNoYearMutiIndex: [0, 0],
      glDateMutiIndex: [90, 0, 0],
      glDateNoYearMutiIndex: [0, 0],
      glYears: [],
      glMonths: [],
      glMonthsNoYear: [],
      glDays: [],
      glDaysNoYear: [],
      nlYears: [],
      nlMonths: [],
      nlMonthsNoYear: [],
      nlDays: [],
      nlDaysNoYear: [],
      animationData: {}, // 动画
      show: false,
    };
    watch = {
      showDatePickerPlus(newVal, oldVal) {
        if (newVal != oldVal) {
          if (newVal)
            this.showDatePicker()
          else
            this.closeDatePicker()
        }
      }
    };
    computed = {
      
    };
    // 生命周期函数，可以为函数，或一个在methods段中定义的方法名
    onLoad() {
      /**
       * 初始化阴阳历数据
       */
      let nlDate = nlDateHandler(this.nlDateMutiIndex);
      let nlDateNoYear = nlDateHandlerNoYear(this.nlDateNoYearMutiIndex);
      let glDateMutiIndex = this.glDateMutiIndex;
      let glDateNoYearMutiIndex = this.glDateNoYearMutiIndex;
      let [y, m, d] = this.initDate.split('-');
      glDateMutiIndex = [glYear.indexOf(parseInt(y)), parseInt(m) - 1, parseInt(d) - 1];
      let glDate = glDateHandler(glDateMutiIndex);
      glDateNoYearMutiIndex = [parseInt(m) - 1, parseInt(d) - 1];
      let glDateNoYear = glDateHandlerNoYear(glDateNoYearMutiIndex);
      // this.setData({
        this.glDateMutiIndex = glDateMutiIndex;
        this.glDateNoYearMutiIndex = glDateNoYearMutiIndex;
        this.glYears = glDate.yearCol;
        this.glMonths = glDate.monthCol;
        this.glDays = glDate.dayCol;
        this.glMonthsNoYear = glDateNoYear.monthCol;
        this.glDaysNoYear = glDateNoYear.dayCol;
        this.nlYears = nlDate.yearCol;
        this.nlMonths = nlDate.monthCol;
        this.nlDays = nlDate.dayCol;
        this.nlMonthsNoYear = nlDateNoYear.monthCol;
        this.nlDaysNoYear = nlDateNoYear.dayCol;
      // })
      // console.log((this.glMonths[this.glDateNoYearMutiIndex[0]] + '-' + this.glDaysNoYear[this.glDateNoYearMutiIndex[1]]));
      // console.log((this.nlMonthsNoYear[this.nlDateNoYearMutiIndex[0]] + this.nlDaysNoYear[this.nlDateNoYearMutiIndex[1]]));
      let calendarYear = (this.glMonths[this.glDateNoYearMutiIndex[0]] + '-' + this.glDaysNoYear[this.glDateNoYearMutiIndex[1]]);
      let lunarCalendar = (this.nlMonthsNoYear[this.nlDateNoYearMutiIndex[0]] + this.nlDaysNoYear[this.nlDateNoYearMutiIndex[1]]);
      this.$emit("currentDate",calendarYear,lunarCalendar)
      this.$apply();
    };

    methods = {
      /**
       * 切换阴阳历类型
       */
      toggleDateType() {
        let dateType = this.datePickerAttr.dateType == 1 ? 2 : 1;
        // this.setData({
        //   ['datePickerAttr.dateType']: dateType
        // })
        this.datePickerAttr.dateType = dateType
      },
      /**
       * 切换是否显示年份
       */
      toggleShowYear() {
        let showYear = this.datePickerAttr.showYear == 1 ? 0 : 1;
        // this.setData({
        //   ['datePickerAttr.showYear']: showYear
        // })
        this.datePickerAttr.showYear = showYear;
      },
      /**
       * 农历数据改变监听
       */
      bindDateNl(e) {
        let value = e.detail.value;
        let data = nlDateHandler(value);
        // this.setData({
          this.nlDateMutiIndex = value;
          this.nlYears = data.yearCol;
          this.nlMonths = data.monthCol;
          this.nlDays = data.dayCol;
        // })
      },
      /**
       * 农历不带年份数据改变监听
       */
      bindDateNlNoYear(e) {
        let value = e.detail.value;
        let data = nlDateHandlerNoYear(value);
        // this.setData({
          this.nlDateNoYearMutiIndex = value;
          this.nlMonthsNoYear = data.monthCol;
          this.nlDaysNoYear = data.dayCol;
          this.$apply();
        // })
      },
      /**
       * 公历数据改变监听
       */
      bindDateGl(e) {
        let value = e.detail.value;
        let data = glDateHandler(value);
        // this.setData({
          this.glDateMutiIndex = value;
          this.glYears = data.yearCol;
          this.glMonths = data.monthCol;
          this.glDays = data.dayCol;
          this.$apply();
        // })
      },
      /**
       * 公历不带年份数据改变监听
       */
      bindDateGlNoYear(e) {
        let value = e.detail.value;
        let data = glDateHandlerNoYear(value);
        // this.setData({
          this.glDateNoYearMutiIndex = value;
          this.glMonthsNoYear = data.monthCol;
          this.glDaysNoYear = data.dayCol;
          this.$apply();
        // })
      },
      /**
       * 选择日期->确定
       */
      submitSelectDate() {
        let date = '';
        if (this.datePickerAttr.dateType == 1) {
          date = this.datePickerAttr.showYear == 1 ? (this.glYears[this.glDateMutiIndex[0]] + '-' + this.glMonths[this.glDateMutiIndex[1]] + '-' + this.glDays[this.glDateMutiIndex[2]]) : (this.glMonths[this.glDateNoYearMutiIndex[0]] + '-' + this.glDaysNoYear[this.glDateNoYearMutiIndex[1]])
        } else {
          date = this.datePickerAttr.showYear == 1 ? (this.nlYears[this.nlDateMutiIndex[0]] + this.nlMonths[this.nlDateMutiIndex[1]] + this.nlDays[this.nlDateMutiIndex[2]]) : (this.nlMonthsNoYear[this.nlDateNoYearMutiIndex[0]] + this.nlDaysNoYear[this.nlDateNoYearMutiIndex[1]])
        }
        // this.closeDatePicker();
        this.show = false;
        this.$apply();
        var myEventDetail = {
          dateStr: date,
          showYear: this.datePickerAttr.showYear == 1 ? true : false,
          dateType: this.datePickerAttr.dateType == 1 ? '公历' : '农历'
        } // detail对象，提供给事件监听函数
        var myEventOption = {} // 触发事件的选项
        this.$emit('submit', myEventDetail, myEventOption)
      },
      onMyButtonTap() {
        this.setData({
          // 更新属性和数据的方法与更新页面数据的方法类似
        })
      },
      _propertyChange(newVal, oldVal) {
      },
      // 显示日期选择控件
      showDatePicker() {
        var animation = wx.createAnimation({
          duration: 160,
          timingFunction: 'linear',
          delay: 0
        })
        this.animation = animation;

        animation.translateY(280).step({
          duration: 0
        })

        this.animationData = animation.export();
        this.show = true;
        this.$apply();
        setTimeout(function () {
          animation.translateY(0).step()
          this.animationData = animation.export();
          this.$apply();
        }.bind(this), 160)
      },
      // 隐藏日期选择控件
      closeDatePicker() {
        this.animation.translateY(280).step();
        this.animationData = this.animation.export();
        this.$apply();
        setTimeout(function () {
          this.show = false;
          this.$apply();
        }.bind(this), 120)
      },
    };
    
  }
</script>
